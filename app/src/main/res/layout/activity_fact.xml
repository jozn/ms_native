<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:showIn="@layout/activity_fact"
    android:orientation="vertical"
    >

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/fact1"
        android:padding="10dp"
        android:background="#eed"
        android:text="FactPhoneAllContacts"
        />


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/rooms"
        android:layout_marginTop="10dp"
        android:padding="10dp"
        android:background="#eed"
        android:text="FactRoomList1"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/msgs"
        android:layout_marginTop="10dp"
        android:padding="10dp"
        android:background="#eed"
        android:text="FactMsgs"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/msgs2"
        android:layout_marginTop="10dp"
        android:padding="10dp"
        android:background="#eed"
        android:text="FactMsgs--Fact4--Insert builder"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/msgs5"
        android:layout_marginTop="10dp"
        android:padding="10dp"
        android:background="#eed"
        android:text="FactMsgs--Fact5--Insert builder -transection"
        />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/msgs6"
        android:layout_marginTop="10dp"
        android:padding="10dp"
        android:background="#eed"
        android:text="FactMsgs--Fact6--normar creatNew -transection"
        />



</LinearLayout>